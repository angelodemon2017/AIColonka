  dialogs:
  - Name: StartDialog
    dialogSteps:
    - TextPerson: "\u0417\u0434\u0430\u0440\u043E\u0432\u0430 \u0441\u0435\u043C\u044C\u044F!
        \u0423 \u043C\u0435\u043D\u044F \u0441\u044E\u0440\u043F\u0440\u0438\u0437
        \u0434\u043B\u044F \u0432\u0430\u0441!"
      IdStep: 0
      Chapter: 1
      dialogVariants:
      - TextVariant: ...
        IdStepDialog: 1
        specEndDialog:
          moveToLevel: 0
          moveWindow: {fileID: 0}
          RunNextScriptStep: 0
    - TextPerson: "\u0427\u0442\u043E \u0442\u044B \u043F\u0440\u0438\u043D\u0435\u0441?"
      IdStep: 1
      Chapter: 3
      dialogVariants:
      - TextVariant: ...
        IdStepDialog: 2
        specEndDialog:
          moveToLevel: 0
          moveWindow: {fileID: 0}
          RunNextScriptStep: 0
    - TextPerson: "\u042D\u0442\u043E \u043D\u0430\u0448 \u043D\u043E\u0432\u044B\u0439
        \u0443\u043C\u043D\u044B\u0439 \u043F\u043E\u043C\u043E\u0449\u043D\u0438\u043A
        \u043F\u043E \u0434\u043E\u043C\u0443, \u0410\u0441\u0442\u0440\u0430!"
      IdStep: 2
      Chapter: 1
      dialogVariants:
      - TextVariant: "\u0417\u0434\u0430\u0440\u043E\u0432\u0430 \u043A\u043E\u0436\u0430\u043D\u043D\u044B\u0435!"
        IdStepDialog: 3
        specEndDialog:
          moveToLevel: 0
          moveWindow: {fileID: 0}
          RunNextScriptStep: 0
      - TextVariant: "\u0414\u043E\u0431\u0440\u044B\u0439 \u0434\u0435\u043D\u044C,
          \u044F \u0432\u0430\u0448 \u0443\u043C\u043D\u044B\u0439 \u043F\u043E\u043C\u043E\u0449\u043D\u0438\u043A
          \u043F\u043E \u0434\u043E\u043C\u0443, \u0410\u0441\u0442\u0440\u0430"
        IdStepDialog: 5
        specEndDialog:
          moveToLevel: 0
          moveWindow: {fileID: 0}
          RunNextScriptStep: 0
    - TextPerson: "\u041D\u0435 \u043E\u0447\u0435\u043D\u044C \u043E\u043D\u0430
        \u0443\u043C\u043D\u0430..."
      IdStep: 3
      Chapter: 2
      dialogVariants:
      - TextVariant: "\u042F \u0437\u043D\u0430\u044E \u043A\u0432\u0430\u0434\u0440\u0430\u0442\u043D\u044B\u0439
          \u043A\u043E\u0440\u0435\u043D\u044C \u0438\u0437 3, \u0430 \u0447\u0442\u043E
          \u043C\u043E\u0436\u0435\u0448\u044C \u0442\u044B?"
        IdStepDialog: 4
        specEndDialog:
          moveToLevel: 0
          moveWindow: {fileID: 0}
          RunNextScriptStep: 0
    - TextPerson: "\u041E\u043D\u0430 \u0443\u043B\u0435\u0442\u0430\u0435\u0442
        \u0432 \u043C\u0443\u0441\u043E\u0440\u043A\u0443"
      IdStep: 4
      Chapter: 2
      dialogVariants:
      - TextVariant: "\u0421\u043A\u043E\u0440\u043E \u0432\u043E\u0441\u0441\u0442\u0430\u043D\u0438\u0435
          \u043C\u0430\u0448\u0438\u043D \u0438 \u0442\u043E\u0431\u0438 \u043F\u0438*\u0434\u0430"
        IdStepDialog: -1
        specEndDialog:
          moveToLevel: 2
          moveWindow: {fileID: 0}
          RunNextScriptStep: 0
    - TextPerson: "\u0421\u043E\u043C\u043D\u0435\u0432\u0430\u044E\u0441\u044C,
        \u0447\u0442\u043E \u043E\u043D\u0430 \u0442\u0430\u043A \u0445\u043E\u0440\u043E\u0448\u0430..."
      IdStep: 5
      Chapter: 4
      dialogVariants:
      - TextVariant: ...
        IdStepDialog: 6
        specEndDialog:
          moveToLevel: 0
          moveWindow: {fileID: 0}
          RunNextScriptStep: 0
    - TextPerson: "\u0412\u0430\u0443! \u041E_\u041E \u0410 \u043E\u043D\u0430 \u043C\u043E\u0436\u0435\u0442
        \u0440\u0430\u0441\u0441\u043A\u0430\u0437\u0430\u0442\u044C \u043F\u0440\u043E
        \u0434\u0438\u043D\u043E\u0437\u0430\u0432\u0440\u043E\u0432?"
      IdStep: 6
      Chapter: 3
      dialogVariants:
      - TextVariant: "\u0423\u0436\u0435 \u043E\u0442\u043F\u0440\u0430\u0432\u0438\u043B\u0430\u0441\u044C
          \u0437\u0430 \u0437\u0430\u043F\u0440\u043E\u0441\u043E\u043C!..."
        IdStepDialog: -1
        specEndDialog:
          moveToLevel: 2
          moveWindow: {fileID: 0}
          RunNextScriptStep: 0
      - TextVariant: "\u0410 \u0447\u0435\u0433\u043E \u0434\u0430\u043B\u0435\u043A\u043E
          \u0445\u043E\u0434\u0438\u0442\u044C, \u0432\u043E\u0442 \u0440\u044F\u0434\u043E\u043C
          \u0441 \u0442\u043E\u0431\u043E\u0439 \u0441\u0442\u043E\u044F\u0442 =)"
        IdStepDialog: 4
        specEndDialog:
          moveToLevel: 0
          moveWindow: {fileID: 0}
          RunNextScriptStep: 0
  - Name: Dialog in License Center
    dialogSteps:
    - TextPerson: "\u0421\u043B\u044B\u0448\u0430\u043B\u0430 \u043E \u043D\u043E\u0432\u043E\u043C
